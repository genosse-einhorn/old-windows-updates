[version]
signature=$chicago$

[DestinationDirs]
RM.driver.dest.copy=30

[install]
copyfiles=RM.driver.dest.copy
updatecfgsys=RM.Sys.upd
updateautobat=RM.Auto.upd

[RM.driver.dest.copy]
VIDE-CDD.SYS

[RM.Sys.upd]
DevAddDev=VIDE-CDD.SYS,device,,"/D:OEMCD001"

[RM.Auto.upd]
CmdAdd=MSCDEX.EXE,"/D:OEMCD001 /L:D"

[SourceDisksNames]
100="%oem.boot.desc%",,5

[SourceDisksFiles]
VIDE-CDD.SYS=100

[Strings]
oem.boot.desc="Windows 95 Setup Bootdisk"
